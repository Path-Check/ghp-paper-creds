{
  "ghp-vaccination:1": {
    "columns": [
      {"path": "credentialSubject.GHPEventRecipient.familyName", "encoder": "string"},
      {"path": "credentialSubject.GHPEventRecipient.givenName", "encoder": "string"},
      {"path": "credentialSubject.GHPEventRecipient.birthDate", "encoder": "isodate-1900-base32"},
      
      {"path": "credentialSubject.GHPVaccinationCertificate.medicinalProductName", "encoder": "string"},
      {"path": "credentialSubject.GHPVaccinationCertificate.marketingAuthorizationHolder", "encoder": "string"},
      {"path": "credentialSubject.GHPVaccinationCertificate.doseNumber", "encoder": "string"},
      {"path": "credentialSubject.GHPVaccinationCertificate.seriesDoses", "encoder": "string"},
      {"path": "credentialSubject.GHPVaccinationCertificate.dateOfVaccination", "encoder": "string"},
      {"path": "credentialSubject.GHPVaccinationCertificate.stateOfVaccination", "encoder": "string"},
      {"path": "credentialSubject.GHPVaccinationCertificate.countryOfVaccination", "encoder": "string"},
      {"path": "credentialSubject.GHPVaccinationCertificate.disease", "encoder": "string"},
      {"path": "credentialSubject.GHPVaccinationCertificate.vaccineDescription", "encoder": "string"},
      {"path": "credentialSubject.GHPVaccinationCertificate.vaccineType", "encoder": "string"},
      
      {"path": "issuanceDate", "encoder": "isodatetime-epoch-base32"},
      {"path": "expirationDate", "encoder": "isodatetime-epoch-base32"},
      {"path": "issuer", "encoder": "string", "prefix": ["did:web:PCF.PW:"]},
      {"path": "id", "encoder": "string"},
      {"path": "proof.created", "encoder": "isodatetime-epoch-base32"},
      {"path": "proof.verificationMethod", "encoder": "string", "prefix": ["did:web:PCF.PW:"]},
      {"path": "proof.proofValue", "encoder": "multibase-base36"}
    ],
    "template": {
      "@context": [
        "https://www.w3.org/2018/credentials/v1",
        "https://w3id.org/security/suites/ed25519-2020/v1"
      ],
      "type": [
        "VerifiableCredential"
      ],
      "credentialSubject": {
        "@context": [ "https://www.demo.com/context/v1" ],
        "type": [ "GHPVaccinationCertificate" ],

        "GHPEventRecipient": {
          "type": [ "GHPEventRecipient" ]
        },
        "GHPVaccinationCertificate": {
          "type": [ "GHPVaccinationCertificate" ]
        }
      },
      "proof": {
        "type": "Ed25519Signature2020",
        "proofPurpose": "assertionMethod"
      }
    }
  }
}